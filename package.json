    "@types/morgan": "^1.7.37",
    "@types/node": "^12.11.1",
    "@types/request": "^2.48.3",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/parser": "^2.4.0",
    "eslint": "^6.5.1",
    "husky": ">=1",
    "jest": "^24.9.0",
    "lint-staged": ">=8",
    "prettier": "1.18.2",
    "supertest": "^4.0.2",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
